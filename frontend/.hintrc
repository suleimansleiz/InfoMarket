{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/structure": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "scrollbar-width",
          "scrollbar-width: none",
          "backdrop-filter"
        ]
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off",
        "select-name": "off"
      }
    ],
    "typescript-config/consistent-casing": "off",
    "typescript-config/strict": "off",
    "no-inline-styles": "off",
    "axe/name-role-value": "off"
  }
}